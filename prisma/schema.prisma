generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Define the model for storing animal locations based on user input
model UserInputAnimal {
  id         Int       @id @default(autoincrement())
  animal     String
  timeInput   DateTime @default(now())
}
